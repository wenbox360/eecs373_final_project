void send_express_scan_command(UART_HandleTypeDef huart) {
	uint8_t packet[9] = {
	        SL_LIDAR_CMD_SYNC_BYTE, SL_LIDAR_CMD_EXPRESS_SCAN,
	        0x05,                    // Payload length
	        0x00,                    // working_mode (0 for legacy express)
	        0x00, 0x00,             // working_flags
	        0x00, 0x00,             // param
	        0x22                     // Checksum
	    };

}


